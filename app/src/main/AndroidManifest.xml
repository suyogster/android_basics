<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidbasics">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ang_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ang_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidBasics">
        <activity android:name=".BookActivity"></activity>
        <activity android:name=".NavigationHost" />
        <activity android:name=".RecyclerView" />
        <activity android:name=".CardViewSpinner" />
        <activity android:name=".ListViewExample" />
        <activity android:name=".RelativeLayout" />
        <activity android:name=".LinearLayout" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>